{
    "version": 2,
    "builds": [
        {
            "src": "backend/server.js",
            "use": "@vercel/node"
        },
        {
            "src": "Global_Sources/package.json",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/backend/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/Global_Sources/$1"
        }
    ]
}